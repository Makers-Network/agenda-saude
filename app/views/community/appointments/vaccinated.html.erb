<div class="container">
  <div class="card">
    <h4 class="card-header" data-cy="vaccinatedPatientText">
      Paciente imunizado:
      <strong>
        <%= current_patient.name %>
      </strong>
    </h4>
    <ul class="list-group list-group-flush">
      <li class="list-group-item">
        <div class="row">
          <%= render "community/doses/doses", doses: @doses %>
        </div>
      </li>
    </ul>
  </div>
</div>
